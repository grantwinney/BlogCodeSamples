<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="appConfiguration">
      <section name="logging"
               type="SampleLibrary.LogSettings, SampleLibrary"
               allowLocation="true"
               allowDefinition="Everywhere" />
    </sectionGroup>
  </configSections>
  <appConfiguration>
    <logging>
      <application level="1"
                   name="logger"
                   logFilePath="C:\ProgramData\MyApp\Logs\"
                   includeDate="false" />
    </logging>
  </appConfiguration>
</configuration>